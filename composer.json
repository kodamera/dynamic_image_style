    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Kodamera AB",
            "email": "info@kodamera.se",
            "homepage": "https://kodamera.se"
        }
    ],
    "homepage": "https://github.com/kodamera/dynamic_image_style",
